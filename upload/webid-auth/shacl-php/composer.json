{
    "name": "k00ni/shacl-php",
    "require": {
        "php": ">=7.0",
        "k00ni/knorke": "dev-master",
        "saft/saft-rdf": "dev-master",
        "saft/saft-sparql": "dev-master",
        "saft/saft-store": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "6.*"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Shacl\\": [ "src/Shacl" ],
            "ShaclPHP\\": [ "src/ShaclPHP" ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": [
                "tests"
            ]
        }
    },
    "minimum-stability": "dev"
}
